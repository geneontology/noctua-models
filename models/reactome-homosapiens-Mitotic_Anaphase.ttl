                                                              <http://www.biopax.org/release/biopax-level3.owl#Pathway> ;
                                                     <http://geneontology.org/lego/hint/layout/x> 2450 ,
                                                                                                  500 ;
                                                     <http://geneontology.org/lego/hint/layout/y> 50 ,
                                                                                                  950 ;
                                                     <http://purl.org/dc/elements/1.1/contributor> "Authored: Orlic-Milacic, M, 2013-01-22" ,
                                                                                                   "Edited: Gillespie, ME, 2013-01-23" ,
                                                                                                   "Reviewed: Gorjánácz, Mátyás, 2013-01-30" ,
                                                                                                   "Reviewed: Mattaj, Iain W, 2013-01-30" ,
                                                                                                   "https://reactome.org/content/detail/R-HSA-68882" ;
                                                     <http://purl.org/dc/elements/1.1/date> "2018-02-27" ;
                                                     <http://purl.org/pav/providedBy> "https://reactome.org" ;
                                                     rdfs:comment "The reassembly of the nuclear envelope (NE) around separated sister chromatids begins in late anaphase and is completed in telophase. Nuclear pore complexes (NPCs) reassemble and insert into the reforming nuclear envelope (reviewed by Guttinger et al. 2009)." ;
                                                     rdfs:label "Nuclear Envelope Reassembly" .


