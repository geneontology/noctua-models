                                                                                         "http://orcid.org/0000-0003-3185-5709"^^xsd:string ;
                                           <http://purl.org/dc/elements/1.1/title> "SYNGO_845"^^xsd:string ;
                                           <http://purl.org/dc/elements/1.1/contributor> "http://orcid.org/0000-0002-0755-5899"^^xsd:string ;
