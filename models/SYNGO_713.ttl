<http://model.geneontology.org/SYNGO_713> rdf:type owl:Ontology ;
                                           <http://geneontology.org/lego/modelstate> "development" ;
                                           <http://purl.org/dc/elements/1.1/contributor> "http://orcid.org/0000-0001-7351-8706"^^xsd:string ,
