                                            <https://w3id.org/biolink/vocab/in_taxon> "http://purl.obolibrary.org/obo/NCBITaxon_10090"^^xsd:string ;
                                            <http://purl.org/dc/elements/1.1/contributor> "https://orcid.org/0000-0002-5676-366X"^^xsd:string ;
                                            <http://purl.org/dc/elements/1.1/date> "2018-02-22"^^xsd:string ;
